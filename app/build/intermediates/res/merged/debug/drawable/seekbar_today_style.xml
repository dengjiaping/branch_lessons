<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:angle="270"
                android:centerColor="#F3F3F3"
                android:centerY="0.75"
                android:endColor="#F3F3F3"
                android:startColor="#F3F3F3" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="#F1312E"
                    android:centerY="0.75"
                    android:endColor="#F1312E"
                    android:startColor="#F1312E" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="#E54E4E"
                    android:centerY="0.75"
                    android:endColor="#E54E4E"
                    android:startColor="#E54E4E" />
            </shape>
        </clip>
    </item>
</layer-list>