<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    android:id="@+id/activity_my_detailed_all"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context="com.yidiankeyan.science.my.activity.MyDetailedAllActivity"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <include layout="@layout/inc_title"></include>

    <View
        android:layout_width="match_parent"
        android:layout_height="20px"
        android:background="#f1f1f1"
        >

    </View>

    <cn.finalteam.loadingviewfinal.PtrClassicFrameLayout
        android:id="@+id/ptr_recommend_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <cn.finalteam.loadingviewfinal.ListViewFinalLoadMore
            android:id="@+id/lv_my_detailed"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:cacheColorHint="#00000000"
            android:layout_marginTop="30px"
            android:divider="@null"
            app:loadMoreMode="scroll"/>

    </cn.finalteam.loadingviewfinal.PtrClassicFrameLayout>

</com.zhy.autolayout.AutoLinearLayout>
